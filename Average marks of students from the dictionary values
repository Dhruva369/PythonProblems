if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
x=student_marks[query_name]
z=0
for y in x:
    z=z+y
'''{2f}".format(float(z/n))'''
m=(z/3)
print("%.2f" % m)

